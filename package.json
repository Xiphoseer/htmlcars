{
  "name": "@xiphoseer/htmlcars",
  "version": "0.1.2",
  "private": false,
  "license": "MPL-2.0",
  "homepage": "https://xiphoseer.github.io/htmlcars",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiphoseer/htmlcars.git"
  },
  "bugs": {
    "url": "https://github.com/xiphoseer/htmlcars/issues"
  },
  "scripts": {
    "build": "vue-cli-service build --target lib --name htmlcars src/index.js",
    "lint": "vue-cli-service lint",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "prepublishOnly": "$npm_execpath run-script build"
  },
  "main": "dist/htmlcars.umd.min.js",
  "unpkg": "dist/htmlcars.umd.min.js",
  "jsdelivr": "dist/htmlcars.umd.min.js",
  "files": [
    "dist/",
    "src/assets/",
    "src/components/",
    "src/*.js"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "js-beautify": "^1.8.9",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "vue-highlightjs": "^1.3.3",
    "vue-resize-directive": "^1.1.4",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.7.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
